{
  "model": "mobilenet_v2",
  "pretrained": true,
  "hw_config_type": "dla",
  "hw_config_subtype": "int5bfp_mixed",
  "input_info": {
    "sample_size": [ 1, 3, 224, 224 ]
  },
  "num_classes": 1000,
  "batch_size": 64,
  "epochs": 8,
  "multiprocessing_distributed": true,
  "optimizer": {
    "type": "Adam",
    "base_lr": 0.00001,
    "schedule_type": "multistep",
    "steps": [
        1,6
    ]
  },
  "compression": {
    "algorithm": "quantization",
    "logfile_name": "int5bfp_mixed_1.0",
    "initializer": {
        "precision": {
            "type": "hawq",
            "bits": [
                5,
                9
            ],
            "num_data_points": 100,
            "iter_number": 200,
            "tolerance": 1e-4,
            "compression_ratio": 1.0,
            "cost_function": "multipass"
        }
    },
    "export_to_onnx_block_floating_point": true
  }
}
