{
  "model": "mobilenet_v2",
  "pretrained": true,
  "hw_config_type": "dla",
  "hw_config_subtype": "int7bfp16",
  "input_info": {
    "sample_size": [ 1, 3, 224, 224 ]
  },
  "num_classes": 1000,
  "batch_size": 80,
  "epochs": 12,
  "multiprocessing_distributed": true,
  "optimizer": {
    "type": "Adam",
    "base_lr": 0.00001,
    "schedule_type": "multistep",
    "steps": [
      3,
      8
    ]
  },
  "compression": {
    "algorithm": "quantization",
    "logfile_name": "int7bfp",
    "export_to_onnx_block_floating_point": false 

  }
}
