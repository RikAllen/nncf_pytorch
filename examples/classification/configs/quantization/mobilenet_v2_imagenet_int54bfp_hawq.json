{
  "model": "mobilenet_v2",
  "pretrained": true,
  "hw_config_type": "dla",
  "hw_config_subtype": "int54bfp_dw",
  "input_info": {
    "sample_size": [ 1, 3, 224, 224 ]
  },
  "num_classes": 1000,
  "batch_size": 64,
  "epochs": 12,
  "multiprocessing_distributed": true,
  "optimizer": {
    "type": "Adam",
    "base_lr": 0.00031,
    "schedule_type": "plateau",
    "scheduler_params": {
      "threshold": 0.1,
      "cooldown": 3
    },
    "weight_decay": 1e-05
  },
  "compression": {
    "algorithm": "quantization",
    "logfile_name": "int54bfp_mixed",
    "initializer": {
        "precision": {
            "type": "hawq",
            "bits": [
                4,
                7
            ],
            "num_data_points": 100,
            "iter_number": 200,
            "tolerance": 1e-4,
            "compression_ratio": 3,
            "cost_function": "multipass"
        }
    },
    "export_to_onnx_block_floating_point": true
  }
}
