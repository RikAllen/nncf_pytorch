{
    "model": "mobilenet_v2",
    "pretrained": true,
    "hw_config_type": "dla",
    "hw_config_subtype": "int7bfp16",
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 100,
    "epochs": 32,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.00001,
        "schedule_type": "plateau",
        "scheduler_params": {
          "threshold": 0.1,
          "cooldown": 3,
          "patience": 3
          }
    },
    "compression": {  
        "algorithm": "quantization",
	"logfile_name": "int7bfp16"
    }
}
