{
  "model": "mobilenet_v2",
  "pretrained": true,
  "hw_config_type": "dla",
  "hw_config_subtype": "int5bfp_mixed",
  "input_info": {
    "sample_size": [ 1, 3, 224, 224 ]
  },
  "num_classes": 1000,
  "batch_size": 64,
  "epochs": 32,
  "multiprocessing_distributed": true,
  "optimizer": {
      "type": "Adam",
      "base_lr": 0.00001,
      "schedule_type": "plateau",
      "scheduler_params": {
        "threshold": 0.1,
        "cooldown": 3,
        "patience": 3
        }
  },
  "compression": {
    "algorithm": "quantization",
    "logfile_name": "int5bfp_mixed_0.8",
    "initializer": {
        "precision": {
            "type": "hawq",
            "bits": [
                5,
                9
            ],
            "num_data_points": 100,
            "iter_number": 200,
            "tolerance": 1e-4,
            "compression_ratio": 0.8,
            "cost_function": "multipass"
        }
    },
    "export_to_onnx_block_floating_point": true
  }
}
